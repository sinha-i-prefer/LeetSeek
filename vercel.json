{
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/index.py"
    }
  ]
}
{
  "crons": [
    {
      "path": "/api/index?source=cron",
      "schedule": "0 */6 * * *"
    }
  ]
}
                
